{
  "name": "lego-components",
  "version": "0.1.0",
  "private": true,
  "main": "dist/lego-components.umd.js",
  "module": "dist/lego-components.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "serve": "vue-cli-service serve",
