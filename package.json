{
  "name": "ns-lego-components",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run clean && npm run build:esm && npm run build:umd",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "build:esm": "rollup --config config/rollup.esm.config.js --bundleConfigAsCjs",
    "build:umd": "rollup --config config/rollup.umd.config.js --bundleConfigAsCjs",
    "clean": "rimraf ./dist",
    "prepublishOnly": "npm run lint && npm run test:unit && npm run build"
  },
  "types": "dist/index.d.ts",
  "main": "dist/ns-lego-components.umd.js",
  "module": "dist/ns-lego-components.esm.js",
  "dependencies": {
    "core-js": "^3.8.3",
    "lodash-es": "^4.17.21"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/jest": "^27.0.1",
    "@types/lodash-es": "^4.17.12",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^2.0.0-0",
    "@vue/vue3-jest": "^27.0.0-alpha.1",
    "babel-jest": "^27.0.6",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "jest": "^27.0.5",
    "less": "^4.0.0",
    "less-loader": "^8.0.0",
    "prettier": "^2.4.1",
    "rimraf": "^5.0.5",
    "rollup": "^4.13.2",
    "rollup-plugin-css-only": "^4.5.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-vue": "^6.0.0",
    "ts-jest": "^27.0.4",
    "typescript": "~4.5.5",
    "vue": "^3.2.13"
  },
  "peerDependencies": {
    "vue": "^3.2.13"
  }
}
